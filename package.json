  "dependencies": {
    "expat-wasm": "3.0.0"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "1.0.5",
    "ava": "^5.2.0",
    "c8": "7.13.0",
    "docdash": "2.0.1",
    "eslint": "8.39.0",
    "jsdoc": "^4.0.2",
    "peggy": "3.0.2"
  }
}
